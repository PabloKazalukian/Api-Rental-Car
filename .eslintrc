{
    "env": {
      "node": true,
      "es6": true
    },
    "extends": "airbnb/base",
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": ["build", "node_modules"]
        }
      }
    },
    "rules": {
       "no-underscore-dangle": 0,
       "no-param-reassign": 0,
       "import/no-unresolved": 0
     }
  }